COLUMN TABLE "HorusDatabase.db.data.METADATA::PRODUCTSCLASSIFICATION" (
	"CLASSIFICATIONID" INTEGER GENERATED BY DEFAULT AS IDENTITY (NO CYCLE NO CACHE NO MINVALUE START WITH 200000000 INCREMENT BY 1 MAXVALUE 2999999999) NOT NULL COMMENT 'ID Product classification',
	"CLASSIFICATIONNAME" NVARCHAR(100) COMMENT 'Classification name',
	PRIMARY KEY ("CLASSIFICATIONID")
)