COLUMN TABLE "HorusDatabase.db.data.HOJASDEGASTO::HOJASDEGASTO.HEADER" (
     "IDHOJADEGASTO" INTEGER GENERATED BY DEFAULT AS IDENTITY (NO CYCLE NO CACHE NO MINVALUE START WITH 200000000 INCREMENT BY 1 MAXVALUE 2999999999) NOT NULL COMMENT 'ID hoja de gasto',
     "NOMBREPACIENTE" NVARCHAR(50) NOT NULL COMMENT 'Nombre Paciente',
     "OJO" NVARCHAR(10) NOT NULL COMMENT 'Ojo a operar',
     "IDCIRUGIA" INTEGER CS_INT NOT NULL COMMENT 'ID Cirugia',
     "FECHAPROCEDIMIENTO" DATE CS_DAYDATE NOT NULL COMMENT 'Fecha del procedimiento',
     "ESTATUS" BOOLEAN NOT NULL COMMENT 'Estado de la hoja de gasto',
	 PRIMARY KEY ("IDHOJADEGASTO")) 
	 WITH ASSOCIATIONS(JOIN "HorusDatabase.db.data.HOJASDEGASTO::CIRUGIAS" AS "CIRUGIAS" ON "CIRUGIAS"."IDCIRUGIA" = "IDCIRUGIA")