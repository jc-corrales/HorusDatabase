COLUMN TABLE "HorusDatabase.db.data.PEDIDOS::PEDIDOS.ITEM" (
     "IDITEM" INTEGER GENERATED BY DEFAULT AS IDENTITY (NO CYCLE NO CACHE NO MINVALUE START WITH 200000000 INCREMENT BY 1 MAXVALUE 2999999999) NOT NULL COMMENT 'ID Pedido',
     "HEADER.IDPEDIDO" INTEGER CS_INT NOT NULL COMMENT 'ID de la orden de pedido asociada',
     "PRODUCTOS.IDPRODUCTO" INTEGER CS_INT NOT NULL COMMENT 'ID del producto a pedir',
     "CANTIDADSOLICITADA" INTEGER NOT NULL COMMENT 'Cantidad Solicitada',
	 PRIMARY KEY ("IDITEM"))
	 WITH ASSOCIATIONS(JOIN "HorusDatabase.db.data.PEDIDOS::PEDIDOS.HEADER" AS "HEADER" ON "HEADER"."IDPEDIDO" = "HEADER.IDPEDIDO",
					   JOIN "HorusDatabase.db.data.METADATA::PRODUCTOS" AS "PRODUCTOS" ON "PRODUCTOS"."IDPRODUCTO" = "PRODUCTOS.IDPRODUCTO"
	 )
	 